{
  "name": "alexmacarthur/laravel-loki-logging",
  "version": "0.0.1",
  "description": "Logging to Loki for Laravel",
  "type": "library",
  "authors": [
    {
      "name": "Sven Mol",
      "email": "s.mol@devcake.nl"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/console": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
    "monolog/monolog": "^3.0",
    "nesbot/carbon": "^2.0|^3.0"
  },
  "autoload": {
    "psr-4": {
      "Devcake\\LaravelLokiLogging\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Devcake\\LaravelLokiLogging\\L3ServiceProvider"
      ]
    }
  }
}
