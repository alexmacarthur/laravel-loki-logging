{
  "name": "alexmacarthur/laravel-loki-logging",
  "version": "0.0.2",
  "description": "Send your Laravel logs to a Grafana Loki server.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Alex MacArthur",
      "email": "dev@macarthur.me",
      "homepage": "https://macarthur.me"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/console": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
    "monolog/monolog": "^3.0",
    "nesbot/carbon": "^2.0|^3.0"
  },
  "autoload": {
    "psr-4": {
      "AlexMacArthur\\LaravelLokiLogging\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AlexMacArthur\\LaravelLokiLogging\\L3ServiceProvider"
      ]
    }
  },
  "require-dev": {
    "laravel/pint": "^1.24"
  }
}
